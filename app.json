{
  "expo": {
    "name": "Travel_UI",
    "slug": "travel_ui",
    "description": "UI made in react native for people who like to Travel and do activities",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mikichachi7.travel_ui"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
